{
    "name": "setemares/nova-announcements",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova",
        "notifications"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "~1.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "SeteMares\\NovaNotifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SeteMares\\NovaNotifications\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SeteMares\\NovaNotifications\\ToolServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
